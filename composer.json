{
    "name": "mcmatters/laravel-helpers",
    "description": "Laravel helpers",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Borzyonok",
            "email": "dima.matters@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "9.* || 10.*",
        "illuminate/database": "9.* || 10.*",
        "illuminate/console": "9.* || 10.*",
        "symfony/http-foundation": "^6.0",
        "symfony/process": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.5.0",
        "orchestra/testbench-core": "^7.5.0"
    },
    "autoload": {
        "psr-4": {
            "McMatters\\Helpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "McMatters\\Helpers\\Tests\\": "tests",
            "McMatters\\Helpers\\Tests\\Mocks\\": "tests/mocks/Classes"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "McMatters\\Helpers\\ServiceProvider"
            ]
        }
    }
}
