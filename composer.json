{
    "name": "mcmatters/laravel-helpers",
    "description": "Laravel helpers",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Borzyonok",
            "email": "dima.matters@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "5.*",
        "illuminate/database": "5.*",
        "illuminate/console": "5.*",
        "symfony/debug": "3.* || 4.*",
        "symfony/http-foundation": "3.* || 4.*",
        "symfony/process": "3.* || 4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.3.0",
        "codeclimate/php-test-reporter": "0.4.4",
        "orchestra/testbench": "3.5.0",
        "orchestra/testbench-core": "3.5.1"
    },
    "autoload": {
        "psr-4": {
            "McMatters\\Helpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "McMatters\\Helpers\\Tests\\": "tests",
            "McMatters\\Helpers\\Tests\\Mocks\\": "tests/mocks/Classes"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "McMatters\\Helpers\\ServiceProvider"
            ]
        }
    }
}
